{
    "name": "One",
    "style": "Dark Custom",
    "author": {
        "name": "Rudd Fawcett",
        "email": "rudd.fawcett@gmail.com"
    },
    "version": "1.0",
    "variables": {
        "boldFont": "not implemented yet"
    },
    "editor": {
        "backgroundColor": "#ffffff",
        "tintColor": "#E55D51"
    },
    "styles": {
        "h1": {
            "font": "Menlo-Bold",
            "color": "#E55D51",
            "size": 40
        },
        "h2": {
            "font": "Menlo-Bold",
            "color": "#E55D51",
            "size": 35
        },
        "h3": {
            "font": "Menlo-Bold",
            "color": "#E55D51",
            "size": 30
        },
        "body": {
            "font": "Courier",
            "size": 22,
            "color": "#A97A00"
        },
        "unordered list": {
            "regex": "^(\t| )*[-*] .*",
            "color": "#009688"
        },
        "ordered list": {
            "regex": "^(\t| )*[0-9]+[.] .*",
            "color": "#009688"
        },
        "bold": {
            "font": "Menlo-Bold",
            "color": "#D19A66"
        },
        "italic": {
            "font": "Menlo-Italic",
            "color": "#B742B3"
        },
        "double": {
            "regex": "[*]{3}.*[*]{3}",
            "font": "Menlo-BoldItalic",
            "color": "#B742B3"
        },
        "code": {
            "color": "#64AF66"
        },
        "url": {
            "color": "#78aedd"
        },
        "image": {
            "color": "#78aedd"
        }
    }
}
